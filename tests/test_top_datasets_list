        \u0438 \u0433\u043E\u0441\u0443\u0434\u0430\u0440\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u043C\u0438
        \u0438\u043D\u0436\u0435\u043D\u0435\u0440\u0430\u043C\u0438 - \u0438\u043D\u0441\u043F\u0435\u043A\u0442\u043E\u0440\u0430\u043C\u0438
        \u043F\u043E \u043D\u0430\u0434\u0437\u043E\u0440\u0443 \u0437\u0430\\n\u0442\u0435\u0445\u043D\u0438\u0447\u0435\u0441\u043A\u0438\u043C
        \u0441\u043E\u0441\u0442\u043E\u044F\u043D\u0438\u0435\u043C \u0438 \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0439
        \u044D\u043A\u0441\u043F\u043B\u0443\u0430\u0442\u0430\u0446\u0438\u0438 \u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D\u043D\u043E\u0439
        \u0442\u0435\u0445\u043D\u0438\u043A\u0438\\n\u041E\u0431\u044A\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u044F.
        \u0420\u044F\u0434\u043E\u043C \u0443 \u0432\u0445\u043E\u0434\u0430 \u043D\u0430
        \u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D \u0434\u043E\u043B\u0436\u0435\u043D
        \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u044C\u0441\u044F \u043E\u043F\u0435\u0440\u0430\u0442\u043E\u0440,
        \u0443 \u043A\u043E\u0442\u043E\u0440\u043E\u0433\u043E\\n\u0434\u043E\u043B\u0436\u043D\u043E
        \u0431\u044B\u0442\u044C \u0443\u0434\u043E\u0441\u0442\u043E\u0432\u0435\u0440\u0435\u043D\u0438\u0435
        \u0441 \u0434\u043E\u043F\u0443\u0441\u043A\u043E\u043C \u043A \u044D\u043A\u0441\u043F\u043B\u0443\u0430\u0442\u0430\u0446\u0438\u0438
        \u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D\u043E\u043C (\u0438\u043B\u0438
        \u0435\u0433\u043E\\n\u043A\u043E\u043F\u0438\u044F), \u0430 \u0442\u0430\u043A\u0436\u0435
        \u0434\u043E\u043B\u0436\u043D\u044B \u0431\u044B\u0442\u044C \u0440\u0430\u0437\u043C\u0435\u0449\u0435\u043D\u044B
        \u043F\u0440\u0430\u0432\u0438\u043B\u0430 \u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u043D\u0438\u044F
        \u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D\u043E\u043C.<o:p></o:p></span></p><p
