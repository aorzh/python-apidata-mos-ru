        style=\\\"font-family: tahoma, geneva, sans-serif;\\\">\u0414\u0430\u0442\u0430-\u0441\u0435\u0442
        \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u0435\u0442 \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u044C
        \u043F\u043E\u0434\u0440\u043E\u0431\u043D\u0443\u044E \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0446\u0438\u044E
        \u043E\u0431 \u043E\u0431\u044A\u0435\u043A\u0442\u0430\u0445, \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C
        \u0438\u0445 \u043C\u0435\u0441\u0442\u043E\u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435
