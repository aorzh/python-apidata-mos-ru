        \u0432 1893 \u0433\u043E\u0434\u0443 \u0438\u043D\u0436\u0435\u043D\u0435\u0440\\n\u0414\u0436\u043E\u0440\u0434\u0436
        \u0412\u0430\u0448\u0438\u043D\u0433\u0442\u043E\u043D \u0413\u0435\u0439\u043B
        \u0424\u0435\u0440\u0440\u0438\u0441-\u043C\u043B\u0430\u0434\u0448\u0438\u0439.
        \u041E\u043D\u043E \u0431\u044B\u043B\u043E \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D\u043E
        \u043D\u0430 \u0412\u0441\u0435\u043C\u0438\u0440\u043D\u043E\u0439\\n\u0432\u044B\u0441\u0442\u0430\u0432\u043A\u0435
        \u0432 \u0427\u0438\u043A\u0430\u0433\u043E. \u041A\u043E\u043B\u0435\u0441\u043E
