        \u043F\u0440\u043E\u043B\u043E\u0436\u0435\u043D\u044B \u0442\u0435\u043F\u043B\u043E\u043A\u043E\u043C\u043C\u0443\u043D\u0438\u043A\u0430\u0446\u0438\u0438
        - \u0442\u0430\u043A \u0441\u043D\u0435\u0433 \u043D\u0435 \u0441\u043A\u0430\u043F\u043B\u0438\u0432\u0430\u0435\u0442\u0441\u044F
        \u043D\u0430 \u0434\u043E\u0440\u043E\u0433\u0430\u0445 \u0438 \u0441\u0440\u0430\u0437\u0443
        \u0442\u0430\u0435\u0442. \u0414\u043E\u0440\u043E\u0433\u0438 \u0431\u0435\u0437
        &laquo;\u043F\u043E\u0434\u043E\u0433\u0440\u0435\u0432\u0430&raquo; \u043E\u0431\u0440\u0430\u0431\u0430\u0442\u044B\u0432\u0430\u044E\u0442
        \u043A\u0438\u043F\u044F\u0442\u043A\u043E\u043C \u0441 \u043F\u0435\u0441\u043A\u043E\u043C:
        \u0433\u043E\u0440\u044F\u0447\u0430\u044F \u0432\u043E\u0434\u0430 \u043F\u043E\u0434\u0442\u043E\u043F\u043B\u044F\u0435\u0442
        \u043B\u0435\u0434, \u0432 \u0442\u043E \u0436\u0435 \u0432\u0440\u0435\u043C\u044F
        \u043F\u0435\u0441\u0447\u0438\u043D\u043A\u0438 \u043F\u0440\u043E\u043D\u0438\u043A\u0430\u044E\u0442
        \u0441\u043A\u0432\u043E\u0437\u044C \u043B\u0435\u0434\u044F\u043D\u043E\u0439
        \u043F\u043E\u043A\u0440\u043E\u0432 \u043A \u0430\u0441\u0444\u0430\u043B\u044C\u0442\u0443,
        \u0430 \u043A\u043E\u0433\u0434\u0430 \u0434\u043E\u0440\u043E\u0433\u0430
