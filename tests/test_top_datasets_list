        \u043A\u0430\u0431\u0438\u043D\u0435 \u0431\u044B\u043B\u043E 20 \u0441\u0438\u0434\u044F\u0447\u0438\u0445
        \u0438 40 \u0441\u0442\u043E\u044F\u0447\u0438\u0445 \u043C\u0435\u0441\u0442,
