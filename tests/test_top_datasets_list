        \u0440\u0435\u0433\u0438\u043E\u043D\u0430\u0445 \u041D\u043E\u0440\u0432\u0435\u0433\u0438\u0438
        \u0432\u043C\u0435\u0441\u0442\u043E \u0440\u0435\u0430\u0433\u0435\u043D\u0442\u043E\u0432
        \u043F\u043E\u0434\u043E\u0433\u0440\u0435\u0432\u0430\u044E\u0442 \u0434\u043E\u0440\u043E\u0433\u0438.
        \u041F\u043E\u0434 \u0442\u0440\u043E\u0442\u0443\u0430\u0440\u0430\u043C\u0438
