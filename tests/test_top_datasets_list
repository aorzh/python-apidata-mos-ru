        \u043E\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043B\u044F\u0435\u0442 \u041E\u0431\u044A\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u0435\\n\u0430\u0434\u043C\u0438\u043D\u0438\u0441\u0442\u0440\u0430\u0442\u0438\u0432\u043D\u043E-\u0442\u0435\u0445\u043D\u0438\u0447\u0435\u0441\u043A\u0438\u0445
        \u0438\u043D\u0441\u043F\u0435\u043A\u0446\u0438\u0439 \u0433\u043E\u0440\u043E\u0434\u0430
        \u041C\u043E\u0441\u043A\u0432\u044B (\u0434\u0430\u043B\u0435\u0435 \u2013
        \u041E\u0431\u044A\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u0435) \u0432\\n\u043B\u0438\u0446\u0435
        \u0441\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u043D\u043E\u0433\u043E \u043F\u043E\u0434\u0440\u0430\u0437\u0434\u0435\u043B\u0435\u043D\u0438\u044F
        - \u0438\u043D\u0441\u043F\u0435\u043A\u0446\u0438\u0438 \u043F\u043E \u043D\u0430\u0434\u0437\u043E\u0440\u0443
        \u0437\u0430 \u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D\u043D\u043E\u0439\\n\u0442\u0435\u0445\u043D\u0438\u043A\u043E\u0439
        \u0432 \u0441\u043E\u0441\u0442\u0430\u0432\u0435 \u041E\u0431\u044A\u0435\u0434\u0438\u043D\u0435\u043D\u0438\u044F
        \u043D\u0430 \u043E\u0441\u043D\u043E\u0432\u0430\u043D\u0438\u0438 \u043F\u043E\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D\u0438\u044F
        \u041F\u0440\u0430\u0432\u0438\u0442\u0435\u043B\u044C\u0441\u0442\u0432\u0430
        \u041C\u043E\u0441\u043A\u0432\u044B\\n\u043E\u0442 13 \u0438\u044E\u043D\u044F
        2006 \u0433. N 392-\u041F\u041F \\\"\u041E\u0431 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0435\u043D\u0438\u0438
        \u0412\u0440\u0435\u043C\u0435\u043D\u043D\u043E\u0433\u043E \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u044F
        \u043E \u043F\u043E\u0440\u044F\u0434\u043A\u0435\\n\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438,
        \u043D\u0430\u0434\u0437\u043E\u0440\u0435 \u0437\u0430 \u0442\u0435\u0445\u043D\u0438\u0447\u0435\u0441\u043A\u0438\u043C
        \u0441\u043E\u0441\u0442\u043E\u044F\u043D\u0438\u0435\u043C \u0438 \u0431\u0435\u0437\u043E\u043F\u0430\u0441\u043D\u043E\u0439
        \u044D\u043A\u0441\u043F\u043B\u0443\u0430\u0442\u0430\u0446\u0438\u0435\u0439\\n\u0430\u0442\u0442\u0440\u0430\u043A\u0446\u0438\u043E\u043D\u043D\u043E\u0439
